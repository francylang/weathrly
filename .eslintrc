{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "jest": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "arrow-body-style": [0],
    "prefer-template": [0],
    "no-else-return": [0],
    "brace-style": [0],
    "no-multiple-empty-lines": [0],
    "no-alert": [0],
    "no-unused-expressions": [0],
    "no-param-reassign": [0],
  }
}
